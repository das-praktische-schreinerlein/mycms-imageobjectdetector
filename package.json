{
    "name": "mycms-imageobjectdetector",
    "version": "1.1.2",
    "license": "Apache-2.0",
    "main": "dist/",
    "types": "dist/",
    "scripts": {
        "build": "npm run prepare-build && tsc",
        "build-run": "npm run build && npm run run-detector",
        "clear-models": "cross-os clean-download-models",
        "download-models": "node dist/detectormodel-downloader.js",
        "download-all-models": "node dist/detectormodel-downloader.js --detectors tfjs_cocossd_mobilenet_v1,tfjs_cocossd_mobilenet_v2,tfjs_cocossd_lite_mobilenet_v2,tfjs_mobilenet_v1,tfjs_posenet,faceapi",
        "prepare": "patch-package",
        "prepare-build": "cross-os prepare-build-os",
        "prepare-build-os": {
            "darwin": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
            "linux": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/mkdirp dist",
            "win32": "node_modules\\.bin\\rimraf dist && node_modules\\.bin\\mkdirp dist"
        },
        "clean-download-models": {
            "darwin": "./node_modules/.bin/rimraf assets/models && ./node_modules/.bin/mkdirp assets/models",
            "linux": "./node_modules/.bin/rimraf assets/models && ./node_modules/.bin/mkdirp assets/models",
            "win32": "node_modules\\.bin\\rimraf assets\\models && node_modules\\.bin\\mkdirp assets\\models"
        },
        "run-imagefile-detector": "node dist/imagefile-objectdetector.js",
        "run-queue-detector": "node dist/queue-objectdetector.js",
        "run-common-imagefile-detector-with-cache": "node dist/imagefile-objectdetector.js --detectors=tfjs_cocossd_mobilenet_v1,tfjs_cocossd_mobilenet_v2,tfjs_cocossd_lite_mobilenet_v2,tfjs_mobilenet_v1,faceapi,picasafile --useDirectoryCache",
        "run-common-imagefile-detector": "node dist/imagefile-objectdetector.js --detectors=tfjs_cocossd_mobilenet_v1,tfjs_cocossd_mobilenet_v2,tfjs_cocossd_lite_mobilenet_v2,tfjs_mobilenet_v1,faceapi,picasafile",
        "run-all-imagefile-detector": "node dist/imagefile-objectdetector.js --detectors=tfjs_cocossd_mobilenet_v1,tfjs_cocossd_mobilenet_v2,tfjs_cocossd_lite_mobilenet_v2,tfjs_mobilenet_v1,tfjs_posenet,faceapi,picasafile",
        "run-all-queue-detector": "node dist/queue-objectdetector.js --detectors=tfjs_cocossd_mobilenet_v1,tfjs_cocossd_mobilenet_v2,tfjs_cocossd_lite_mobilenet_v2,tfjs_mobilenet_v1,tfjs_posenet,faceapi,picasafile",
        "run-all-queue-detector-with-cache": "node dist/queue-objectdetector.js --detectors=tfjs_cocossd_mobilenet_v1,tfjs_cocossd_mobilenet_v2,tfjs_cocossd_lite_mobilenet_v2,tfjs_mobilenet_v1,tfjs_posenet,faceapi,picasafile --useDirectoryCache",
        "test": "echo 'no tests'"
    },
    "private": true,
    "dependencies": {
        "@dps/mycms-commons": "git://github.com/das-praktische-schreinerlein/mycms-commons.git#4.3.0",
        "@tensorflow-models/coco-ssd": "1.0.0",
        "@tensorflow-models/mobilenet": "1.0.0",
        "@tensorflow-models/posenet": "1.0.0",
        "@tensorflow/tfjs": "1.0.1",
        "@tensorflow/tfjs-node": "0.3.1",
        "@tensorflow/tfjs-node-gpu": "0.3.1",
        "canvas": "~2.4.1",
        "domino": "~1.0.30",
        "download": "~7.1.0",
        "exif": "~0.6.0",
        "face-api.js": "0.18.0",
        "jpeg-js": "~0.3.4",
        "minimist": "~1.2.0",
        "mkdirp": "~0.5.1",
        "node-fetch": "~2.3.0",
        "promise-serial": "~0.1.5",
        "readdirp": "~2.1.0",
        "rsmq": "~0.10.0",
        "rsmq-worker": "~0.5.2",
        "xmlhttprequest": "~1.8.0"
    },
    "devDependencies": {
        "@compodoc/compodoc": "~1.1.5",
        "@types/fluent-ffmpeg": "~2.1.6",
        "@types/node": "~8.10.26",
        "cross-os": "~1.3.0",
        "enhanced-resolve": "~4.1.0",
        "patch-package": "~5.1.1",
        "rimraf": "~2.6.2",
        "ts-node": "~2.0.0",
        "tslint": "~4.5.1",
        "typescript": "~3.2.4"
    }
}
